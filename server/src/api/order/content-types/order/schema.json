{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "customerInfo": {
      "displayName": "Customer Info",
      "type": "component",
      "repeatable": false,
      "component": "elements.customer-info"
    },
    "dateReceived": {
      "type": "date"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "RECIEVED",
        "PROCESSING",
        "FULFILLED"
      ]
    },
    "orderItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    }
  }
}
